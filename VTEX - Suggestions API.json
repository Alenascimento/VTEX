						"required": true,
						"style": "simple",
						"schema": {
							"type": "string",
							"example": "application/vnd.vtex.suggestion.v0+json",
                            "default": "application/json"
						}
					},
                    {
                        "name": "Content-Type",
                        "in": "header",
