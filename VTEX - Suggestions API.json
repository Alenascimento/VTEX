								"ProductId": 321,
								"ProductName": "Product sample",
								"NameComplete": "My complete product name?",
								"ProductDescription": "sample",
								"BrandName": "Brand 1",
								"SkuName": "Sku sample",
								"SellerId": null,
								"Height": 1,
								"Width": 1,
								"Length": 1,
								"WeightKg": 1,
								"Updated": null,
								"RefId": "REFID123",
