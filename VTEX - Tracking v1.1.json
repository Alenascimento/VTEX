  "paths": {
    "/v1.1/auth": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "summary": "Asynchronous Login",
        "description": "This endpoint permits the user to acquire the mandatory authentication to access the VTEX Tracking endpoints.",
        "parameters": [
        {
            "name": "Content-Type",
            "in": "header",
            "description": "Type of the content being sent",
            "required": true,
            "style": "simple",
            "schema": {
                "type": "string",
                "default": "application/json"
            }
        },
        {
            "name": "Accept",
            "in": "header",
            "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand",
            "required": true,
            "style": "simple",
            "schema": {
                "type": "string",
                "default": "application/json"
            }
        }
        ],
        "requestBody": {
          "description": "",
          "content": {
              "application/json": {
                  "schema": {
                      "type": "object",
                      "title": "Request body",
                      "default": {},
                      "required": [
                          "username",
                          "password"
                      ],
                      "properties": {
                        "username": {
                          "type": "string",
                          "description": "Add client's username",
                          "default": "username"
                        },
                        "password": {
                          "type": "string",
                          "description": "Add client's password",
                          "default": "password"
                        }
                      }
                    }
                  }
                }
              },
        "responses": {
          "200": {
            "description": "Success",
            "headers": {},
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/TokenResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        },
        "deprecated": false
      }
    },
    "/v1.1/servicos": {
      "get": {
        "tags": [
          "Delivery Services"
        ],
        "summary": "Get Delivery Services List",
        "description": "Retrieves a list of delivery services created by date",
        "parameters": [
        {
          "name": "Content-Type",
          "in": "header",
          "description": "Type of the content being sent",
          "required": true,
          "style": "simple",
          "schema": {
              "type": "string",
              "default": "application/json"
          }
        },
        {
          "name": "Accept",
          "in": "header",
          "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand",
          "required": true,
          "style": "simple",
          "schema": {
              "type": "string",
              "default": "application/json"
          }
        },
          {
            "name": "status",
            "in": "query",
            "description": "Current status of the delivery service",
            "style": "form",
            "schema": {
              "type": "string",
              "default": ""
            }
          },
          {
            "name": "dateInit",
            "in": "query",
            "description": "Initial date for the desired request. The format is date-time.",
            "style": "form",
            "schema": {
              "type": "string",
              "format": "date-time",
              "default":"2013-10-21T13:28:06.419Z"
            }
          },
          {
            "name": "dateEnd",
            "in": "query",
            "description": "End date for the desired request. The format is date-time",
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "format": "date-time",
              "default": "2013-10-21T13:28:06.419Z"
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "Number of items shown per page",
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 40
            }
          },
          {
            "name": "page",
            "in": "query",
            "description": "Number of pages desired for the request",
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 1
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "headers": {},
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/PaginationResult%5BDeliveryServiceResponse_v1_1%5D"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "deprecated": false
      }
    },
